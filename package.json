{
  "name": "papa-js",
  "version": "1.0.0",
  "description": "Factory library for Javascript testing",
  "main": "index.js",
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "test": "mocha --compilers js:babel-core/register --colors 'src/**/*.spec.js'",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w 'src/**/*.spec.js'",
    "testx": "mocha --require test/register-babel.js --compilers js:babel-register 'test/**/*spec.js'",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ndp/papa-js.git"
  },
  "keywords": [
    "testing",
    "factory"
  ],
  "author": "Andrew J. Peterson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndp/papa.js/issues"
  },
  "homepage": "https://github.com/ndp/papa-js#readme",
  "devDependencies": {
    "babel": "^6.5",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "^6.2",
    "babel-plugin-add-module-exports": "*",
    "babel-preset-es2015": "^6.14",
    "chai": "^3.5.0",
    "eslint": "*",
    "eslint-loader": "*",
    "mocha": "^3.0.2",
    "webpack": "^2.1.0-beta.25"
  }
}
